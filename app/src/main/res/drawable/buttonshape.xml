<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#fdfbfb"></solid>
            <corners android:radius="70dp"></corners>
            <padding android:bottom="5pt" android:top="5pt" android:left="6pt" android:right="6pt"></padding>
            <stroke android:color="#fdfbfb" android:width="4dp"></stroke>


        </shape>
    </item>
    <item >

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#050303"></solid>
            <corners android:radius="50dp"></corners>
            <padding android:bottom="5pt" android:top="5pt" android:left="6pt" android:right="6pt"></padding>
            <stroke android:color="#fdfbfb" android:width="2dp"></stroke>
        </shape>

    </item>


</selector>
